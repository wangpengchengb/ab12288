                      <view><span style="color:#333">{{item.actor.name}}</span> 更改了Issue <span
                          style="color:#333">{{item.repo.human_name}}</span> 为进行中</view>
                      <view><span style="color:#333">{{item.payload.title}}</span></view>
                    </view>
