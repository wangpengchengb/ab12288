                    <span style="color:#333">{{item.actor.name}}</span> Fork了 <span
                      style="color:#333">{{item.repo.human_name}}</span> 到 <span
                      style="color:#333">{{item.payload.human_name}}</span>
                  </view>
                </view>
                <view wx:if="{{item.type=='CommitCommentEvent'}}">
