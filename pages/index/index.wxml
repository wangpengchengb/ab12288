                      <view><span style="color:#333">{{item.actor.name}}</span> 关闭了 <span
                          style="color:#333">{{item.repo.human_name}}</span> 的Issue</view>
                      <view><span style="color:#333">{{item.payload.title}}</span></view>
                    </view>
                  </block>
