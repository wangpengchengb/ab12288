                    <view><span style="color:#333">{{item.actor.name}}</span> 评论了 <span
                        style="color:#333">{{item.repo.human_name}}</span> 的Commit</view>
                    <view><span style="color:#333">{{item.payload.comment.body}}</span></view>
                  </view>
                </view>
